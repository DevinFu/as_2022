<application>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="100" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="3" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="ObjectiveC" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="BIDI_TEXT_DIRECTION" value="RTL" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <multiLine>false</multiLine>
    <pattern pattern="\btodo:[\s\S]*" />
    <pattern icon="important" pattern="\bfixme:[\s\S]*" />
    <pattern icon="question" useCustomColors="true" pattern="\bpoint:[\s\S]*">
      <option name="FOREGROUND" value="c05e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bgood:[\s\S]*">
      <option name="FOREGROUND" value="c05e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern icon="question" pattern="\bcfs:[\s\S]*" />
    <pattern icon="important" useCustomColors="true" pattern="\bbad:[\s\S]*">
      <option name="FOREGROUND" value="c00084" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" pattern="\btip:[\s\S]*">
      <option name="FOREGROUND" value="95b9e6" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="1717e6" />
    </pattern>
    <filter name="todo" />
    <filter name="study">
      <pattern index="5" />
      <pattern index="6" />
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
</application>